var count = 0;

function cc(card) {
  // Only change code below this line
if(card <= 2 || card <= 6)
{
   count++;
}
if(card >= 10 || card === "J" || card ==="K"|| card ==="Q" || card === "A"){
  count--;
}

let bet = "Bet"
if(count <= 0 ){
  bet = "Hold"
}
return count + " " + bet

  // Only change code above this line
}
